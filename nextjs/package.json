{
  "name": "@futurebrand/helpers-nextjs",
  "version": "0.3.0",
  "description": "FutureBrand Helpers",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/futurebrand/helpers.git"
  },
  "author": "Gerson Lima",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/futurebrand/helpers/issues"
  },
  "homepage": "https://github.com/futurebrand/helpers#readme",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    "./plugins": "./dist/plugins/index.js",
    "./components": "./dist/components/index.js",
    "./configs": "./dist/configs/index.js",
    "./hooks": "./dist/hooks/index.js",
    "./middlewares": "./dist/middlewares/index.js",
    "./modules": "./dist/modules/index.js",
    "./services": "./dist/services/index.js",
    "./utils": "./dist/utils/index.js",
    "./router": "./dist/router/index.js",
    "./contexts": "./dist/contexts/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "components": [
        "dist/components/index.d.ts"
      ],
      "configs": [
        "dist/configs/index.d.ts"
      ],
      "hooks": [
        "dist/hooks/index.d.ts"
      ],
      "middlewares": [
        "dist/middlewares/index.d.ts"
      ],
      "modules": [
        "dist/modules/index.d.ts"
      ],
      "services": [
        "dist/services/index.d.ts"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ],
      "contexts": [
        "dist/contexts/index.d.ts"
      ],
      "router": [
        "dist/router/index.d.ts"
      ],
      "plugins": [
        "dist/plugins/types.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.26",
    "@types/react": "^18.2.65",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "next": "^14.1.3",
    "next-intl": "^3.9.4",
    "path-to-regexp": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-variants": "^0.2.1"
  }
}
