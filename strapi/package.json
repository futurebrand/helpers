{
  "name": "@futurebrand/helpers-strapi",
  "version": "1.2.3",
  "description": "This is the description of the plugin.",
  "strapi": {
    "name": "futurebrand-strapi-helpers",
    "description": "Description of futurebrand-strapi-helpers plugin",
    "kind": "plugin"
  },
  "scripts": {
    "build:lib": "tsc -p lib/tsconfig.build.json && tsc-alias -p lib/tsconfig.build.json",
    "build:plugin": "tsx build.ts",
    "build": "npm run build:plugin && npm run build:lib",
    "verify": "strapi-plugin verify",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link"
  },
  "main": "dist/lib/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.tsx",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./configs": {
      "types": "./dist/lib/configs/index.d.ts",
      "source": "./lib/configs/index.ts",
      "import": "./dist/lib/configs/index.mjs",
      "require": "./dist/lib/configs/index.js",
      "default": "./dist/lib/configs/index.js"
    },
    "./modules": {
      "types": "./dist/lib/modules/index.d.ts",
      "source": "./lib/modules/index.ts",
      "import": "./dist/lib/modules/index.mjs",
      "require": "./dist/lib/modules/index.js",
      "default": "./dist/lib/modules/index.js"
    },
    "./services": {
      "types": "./dist/lib/services/index.d.ts",
      "source": "./lib/services/index.ts",
      "import": "./dist/lib/services/index.mjs",
      "require": "./dist/lib/services/index.js",
      "default": "./dist/lib/services/index.js"
    },
    "./types": {
      "types": "./dist/lib/types/index.d.ts",
      "source": "./lib/types/index.ts",
      "import": "./dist/lib/types/index.mjs",
      "require": "./dist/lib/types/index.js",
      "default": "./dist/lib/types/index.js"
    },
    "./utils": {
      "types": "./dist/lib/utils/index.d.ts",
      "source": "./lib/utils/index.ts",
      "import": "./dist/lib/utils/index.mjs",
      "require": "./dist/lib/utils/index.js",
      "default": "./dist/lib/utils/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "modules": [
        "dist/lib/modules/index.d.ts"
      ],
      "services": [
        "dist/lib/services/index.d.ts"
      ],
      "configs": [
        "dist/lib/configs/index.d.ts"
      ],
      "utils": [
        "dist/lib/utils/index.d.ts"
      ],
      "types": [
        "dist/lib/types/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.11",
    "@strapi/icons": "^2.0.0-rc.11",
    "@strapi/strapi": "^5.0.2",
    "immer": "^10.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-intl": "^6.7.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@strapi/pack-up": "^5.0.1",
    "@strapi/sdk-plugin": "^5.2.6",
    "@strapi/typescript-utils": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "rollup-plugin-node-externals": "^7.1.3",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "5.6.2",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "maintainers": [
    {
      "name": "A Strapi developer"
    }
  ],
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}